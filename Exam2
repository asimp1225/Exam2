def maxdel (x):
    y = list(x)
    output=list(x)
    gtrthn=bool()
    for i in range(len(y)-1,0,-1):
        for n in range(len(output)-1,0,-1):
            if output[n]>y[i]:
                gtrthn = True
            else:
                gtrthn = False
            if gtrthn == True:
                output.remove(output[n])
                break
        return output
                
maxdel ([1,3,2,5,4,6,5])
